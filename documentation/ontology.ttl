#################################################################
#    Object Properties
#################################################################

###  https://schema.org/aggregateRating
<https://schema.org/aggregateRating> rdf:type owl:ObjectProperty ;
                                     rdfs:range <https://schema.org/AggregateRating> ;
                                     rdfs:comment "The overall rating, based on a collection of reviews or ratings, of the item." ;
                                     rdfs:label "aggregateRating" .


###  https://schema.org/geo
<https://schema.org/geo> rdf:type owl:ObjectProperty ;
                         rdfs:domain <https://schema.org/City> ;
                         rdfs:range <https://schema.org/GeoCoordinates> ;
                         rdfs:comment "Specifies the coordinates of a place" ;
                         rdfs:label "geo" .


###  https://schema.org/location
<https://schema.org/location> rdf:type owl:ObjectProperty ;
                              rdfs:range <https://schema.org/City> ;
                              rdfs:comment "The location of, for example, where an event is happening, where an organization is located, or where an action takes place." ;
                              rdfs:label "location" .


###  https://schema.org/priceSpecification
<https://schema.org/priceSpecification> rdf:type owl:ObjectProperty ;
                                        rdfs:range <https://schema.org/PriceSpecification> ;
                                        rdfs:comment "One or more detailed price specifications, indicating the unit price and delivery or payment charges." ;
                                        rdfs:label "priceSpecification" .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/plabradorr/ontologies/2025/3/ITO/isRecommended
:isRecommended rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:boolean ;
               rdfs:comment "Boolean to specify if AggregateRating > 4, defaults at false." ;
               rdfs:label "isRecommended" .


###  https://schema.org/identifier
<https://schema.org/identifier> rdf:type owl:DatatypeProperty ;
                                rdfs:domain owl:Thing ;
                                rdfs:range xsd:integer ;
                                rdfs:comment "The identifier property represents any kind of identifier for any kind of Thing, such as ISBNs, GTIN codes, UUIDs etc." ;
                                rdfs:label "identifier" .


###  https://schema.org/latitude
<https://schema.org/latitude> rdf:type owl:DatatypeProperty ;
                              rdfs:domain <https://schema.org/GeoCoordinates> ;
                              rdfs:range xsd:decimal ;
                              rdfs:comment "The latitude of a location. For example 37.42242" ;
                              rdfs:label "latitude" .


###  https://schema.org/longitude
<https://schema.org/longitude> rdf:type owl:DatatypeProperty ;
                               rdfs:domain <https://schema.org/GeoCoordinates> ;
                               rdfs:range xsd:decimal ;
                               rdfs:comment "The longitude of a location. For example -122.08585" ;
                               rdfs:label "longitude" .


###  https://schema.org/name
<https://schema.org/name> rdf:type owl:DatatypeProperty ;
                          rdfs:domain owl:Thing ;
                          rdfs:range xsd:string ;
                          rdfs:comment "The name of the item." ;
                          rdfs:label "name" .


###  https://schema.org/price
<https://schema.org/price> rdf:type owl:DatatypeProperty ;
                           rdfs:domain <https://schema.org/PriceSpecification> ;
                           rdfs:range xsd:decimal ;
                           rdfs:comment "The offer price of a product, or of a price component when attached to PriceSpecification and its subtypes." ;
                           rdfs:label "price" .


###  https://schema.org/priceCurrency
<https://schema.org/priceCurrency> rdf:type owl:DatatypeProperty ;
                                   rdfs:domain <https://schema.org/PriceSpecification> ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "The currency of the price, or a price component when attached to PriceSpecification and its subtypes." ;
                                   rdfs:label "priceCurrency" .


###  https://schema.org/ratingValue
<https://schema.org/ratingValue> rdf:type owl:DatatypeProperty ;
                                 rdfs:domain <https://schema.org/AggregateRating> ;
                                 rdfs:range xsd:decimal ;
                                 rdfs:comment "The rating for the content." ;
                                 rdfs:label "ratingValue" .


###  https://schema.org/starRating
<https://schema.org/starRating> rdf:type owl:DatatypeProperty ;
                                rdfs:domain <https://schema.org/Hotel> ;
                                rdfs:range xsd:integer ;
                                rdfs:comment "An official rating for a lodging business or food establishment, e.g. from national associations or standards bodies." ;
                                rdfs:label "starRating" .


###  https://schema.org/startDate
<https://schema.org/startDate> rdf:type owl:DatatypeProperty ,
                                        owl:FunctionalProperty ;
                               rdfs:domain <https://schema.org/Event> ;
                               rdfs:range xsd:dateTime ;
                               rdfs:comment "The start date and time of the item." ;
                               rdfs:label "startDate" .


###  https://schema.org/unitText
<https://schema.org/unitText> rdf:type owl:DatatypeProperty ;
                              rdfs:domain <https://schema.org/PriceSpecification> ;
                              rdfs:range xsd:string ;
                              rdfs:comment "A string or text indicating the unit of measurement." ;
                              rdfs:label "unitText" .


