###  https://schema.org/AggregateRating
<https://schema.org/AggregateRating> rdf:type owl:Class ;
                                     rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                       owl:onProperty <https://schema.org/ratingValue> ;
                                                       owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
                                                       owl:onDataRange xsd:decimal
                                                     ] ,
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty <https://schema.org/ratingValue> ;
                                                       owl:maxQualifiedCardinality "5"^^xsd:nonNegativeInteger ;
                                                       owl:onDataRange xsd:decimal
                                                     ] ;
                                     rdfs:comment "The average rating based on multiple ratings or reviews." ;
                                     rdfs:label "Aggregate Rating" .


